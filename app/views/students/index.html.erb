

<body style="background-color:#ffff99;"><center>

<h1>Students</h1>
<button><%=link_to("Add new Student",'students/new',:class=>'action new')%></button>

<table class=" table table-dark " summary="Subject list" style="border:1px solid;">
<tr class="header" style="border:1px solid;">
<%# <th style="border:1px solid;">id</th> %>
<th style="border:1px solid;">name</th>
<th style="border:1px solid;">email</th>
<th style="border:1px solid;">phone</th>
<th style="border:1px solid;">Action</th>
</tr>
<h1><% @students.each %>  </h1>
<% @students.each do |student|%>
<tr style="border:1px solid;">
<%# <td style="border:1px solid;"> <%= student.id  %>
<td style="border:1px solid;"><%= student.name  %></td>
<td class='center' style="border:1px solid;"><%= student.email  %></td>
<td class='center' style="border:1px solid;"><%= student.phone %></td><br/>
<td class="action" style="border:1px solid;">
  <button> <%=link_to("Show",student_path(student.id),:class=>'action show')%></button>
  <button> <%=link_to("Edit",edit_student_path(student.id),:class=>'action edit')%></button>
   <button><%=link_to("Delete",delete_student_path(student.id),:class=>'action delete')%></button>
</td>

</tr>
<%end%>
</table>

</center>


</body>